import java.util.*;
import java.io.*;

public class Driver {
    public static void main(String[] args) {
        Mage[] mage = new Mage[10];
        Random r = new Random();
	for(int i =0; i<mage.length; i++){
	    String name = "";
	    name += (char)('A' + r.nextInt(26));
	    mage[i] = new Mage();
	    mage[i].setName(name);
	}
	System.out.println(Arrays.toString(ma));
